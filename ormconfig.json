{
   "type": "sqlite",
   "database": "delmar.db",
   "synchronize": true,
   "logging": false,
   "entities": [
      "./build/entity/**/*.js"
   ],
   "migrations": [
      "./src/migration/**/*.js"
   ],
   "subscribers": [
      "./src/subscriber/**/*.js"
   ],
   "cli": {
      "entitiesDir": "./src/entity",
      "migrationsDir": "./src/migration",
      "subscribersDir": "./src/subscriber"
   }
}